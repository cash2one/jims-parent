<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jims.asepsis.dao.AsepsisDetailDictDao">

    <sql id="asepsisDetailDictColumns">
        "ID" as "id"
        ,"ASEPSIS_CODE" as "asepsisCode"
        ,"ITEM_CODE" as "itemCode"
        ,"ITEM_NAME" as "itemName"
        ,"ITEM_SPEC" as "itemSpec"
        ,"AMOUNT" as "amount"
        ,"UNITS" as "units"
        ,"ITEM_PRICE" as "itemPrice"
        ,"STORAGE" as "storage"
        ,"SUB_STORAGE" as "subStorage"
        ,"MEMOS" as "memos"
        ,"FIRM_ID" as "firmId"
        ,"ORG_ID" as "orgId"
    </sql>

    <select id="get" resultType="AsepsisDetailDict">
        SELECT
        <include refid="asepsisDetailDictColumns"/>
        FROM ASEPSIS_DETAIL_DICT a
        WHERE a.id = #{id}
    </select>

    <select id="findList" resultType="AsepsisDetailDict">
        SELECT
        <include refid="asepsisDetailDictColumns"/>
        FROM ASEPSIS_DETAIL_DICT a
        <where>
            a.org_id =  #{orgId}
        </where>
    </select>

    <select id="findAllList" resultType="AsepsisDetailDict">
        SELECT
        <include refid="asepsisDetailDictColumns"/>
        FROM ASEPSIS_DETAIL_DICT a
    </select>

    <insert id="insert">
        INSERT INTO ASEPSIS_DETAIL_DICT(
            ID
            ,ASEPSIS_CODE
            ,ITEM_CODE
            ,ITEM_NAME
            ,ITEM_SPEC
            ,AMOUNT
            ,UNITS
            ,ITEM_PRICE
            ,STORAGE
            ,SUB_STORAGE
            ,MEMOS
            ,FIRM_ID
            ,ORG_ID
        ) VALUES (
            #{id}
            ,#{asepsisCode}
            ,#{itemCode}
            ,#{itemName}
            ,#{itemSpec}
            ,#{amount}
            ,#{units}
            ,#{itemPrice}
            ,#{storage}
            ,#{subStorage}
            ,#{memos}
            ,#{firmId}
            ,#{orgId}
        )
    </insert>

    <update id="update">
        UPDATE ASEPSIS_DETAIL_DICT SET
            ID = #{id}
            ,ASEPSIS_CODE = #{asepsisCode}
            ,ITEM_CODE = #{itemCode}
            ,ITEM_NAME = #{itemName}
            ,ITEM_SPEC = #{itemSpec}
            ,AMOUNT = #{amount}
            ,UNITS = #{units}
            ,ITEM_PRICE = #{itemPrice}
            ,STORAGE = #{storage}
            ,SUB_STORAGE = #{subStorage}
            ,MEMOS = #{memos}
            ,FIRM_ID = #{firmId}
            ,ORG_ID = #{orgId}
        WHERE id = #{id}
    </update>

    <delete id="delete">
        delete FROM ASEPSIS_DETAIL_DICT
        WHERE id = #{id}
    </delete>

    <delete id="deleteByCode">
        delete FROM ASEPSIS_DETAIL_DICT
        WHERE ASEPSIS_CODE = #{asepsisCode}
    </delete>
</mapper>
